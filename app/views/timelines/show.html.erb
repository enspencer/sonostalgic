<% if @timeline.title %>
<h1 class='timeline-id' id='<%= @timeline.id %>'><%= @timeline.title %></h1>
<% else %>
<h1 class='timeline-id' id='<%= @timeline.id %>'>Timeline id:<%= @timeline.id %></h1>
<% end %>

<span class='btn btn-primary share-button'>Share With Friends</span>

<h2 class='timeline-birthyear'>Born: <%= @timeline.birthyear %></h2>
<div class='spacer'></div>
<div id='life'>
</div>
<script src="graph.js"></script>
<script src="jquery.tipsy.js"></script>